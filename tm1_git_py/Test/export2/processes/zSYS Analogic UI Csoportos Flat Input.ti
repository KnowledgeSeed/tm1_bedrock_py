i = nStartIndex;
While(i <= nEndIndex);
    sInputPeriod = SubsetGetElementName(sPeriodDim, sAllNTimeSubset, i);
    CellPutS(pCurrency, 'Channel Time Dependent Attributes', pVersion, sInputPeriod, pCsoportosFlat, 'Currency');
    
    CellPutN(0, 'Channel Cost', pVersion, sInputPeriod, pCsoportosFlat, 'Nem tartozik termekhez', 'HUF', 'CS.FL', 'Value');
    CellPutN(0, 'Channel Cost', pVersion, sInputPeriod, pCsoportosFlat, 'Nem tartozik termekhez', 'EUR', 'CS.FL', 'Value');
    CellPutN(0, 'Channel Cost', pVersion, sInputPeriod, pCsoportosFlat, 'Nem tartozik termekhez', 'USD', 'CS.FL', 'Value');
    i = i+1;
END;


i = nStartIndex;
While(i <= nEndIndex);
    sInputPeriod = SubsetGetElementName(sPeriodDim, sAllNTimeSubset, i);
    pNewValue = StringToNumber(pValue)\12;
    CellPutN(pNewValue, 'Channel Cost', pVersion, sInputPeriod, pCsoportosFlat, 'Nem tartozik termekhez', pCurrency, 'CS.FL', 'Value');
    i = i+1;
