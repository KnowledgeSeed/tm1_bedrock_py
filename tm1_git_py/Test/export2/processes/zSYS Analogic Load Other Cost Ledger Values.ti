pLedgerDimensionName = 'ledger';
sSubsetName_OtherCostETL = 'OtherCostETL';
sMDX = '
{
	TM1FILTERBYLEVEL({
		TM1DRILLDOWNMEMBER({
			[ledger].[ledger].[Other Cost]
		}, ALL, RECURSIVE)
	}, 0)
}
';

HierarchySubsetCreate(pLedgerDimensionName, pLedgerDimensionName, sSubsetName_OtherCostETL, 1);
HierarchySubsetMDXSet(pLedgerDimensionName, pLedgerDimensionName, sSubsetName_OtherCostETL, sMDX);


# Mody 10.14.
# last year mindent ki kell nullázni utána felül lehet írni
# all periodnál minden törlése az adott kockában


pLedgerDimensionName = 'ledger';
sOtherCostAllN = 'OtherCostAllNElement';

# subsetet atirni
sMDXOtherCostAllN = '
{
	TM1FILTERBYLEVEL({
		TM1DRILLDOWNMEMBER({
			[ledger].[ledger].[Other Cost]
		}, ALL, RECURSIVE)
	}, 0)
}
	';
#
	
HierarchySubsetCreate(pLedgerDimensionName, pLedgerDimensionName, sOtherCostAllN, 1);
HierarchySubsetMDXSet(pLedgerDimensionName, pLedgerDimensionName, sOtherCostAllN, sMDXOtherCostAllN);
nStartLedger = 1;
nMaxledgerLen = SubsetGetSize(pLedgerDimensionName, sOtherCostAllN);
j = nStartLedger;
While( j <= nMaxledgerLen);
  sInputLedger = SubsetGetElementName(pLedgerDimensionName, sOtherCostAllN, j);
    #IF( pLastYear = 1);
      sPeriodSubset = 'Last_Actual_Year';
    #ENDIF;
#    IF( pAllPeriod = 1);
#      sPeriodSubset = 'All N';
#    ENDIF;
      sPeriodDim = 'Period';
      nStartPeriod = 1;
      nMaxPeriodLen = SubsetGetSize(sPeriodDim, sPeriodSubset);
      i = nStartPeriod;
      While(i <= nMaxPeriodLen);
      sInputPeriod = SubsetGetElementName(sPeriodDim, sPeriodSubset, i);
      
      # kockat atirni
      CellPutN(0, 'Other Cost', pVersion, sInputPeriod, sInputLedger, 'Cost', 'Value');
      i = i+1;
    END;
j = j+ 1;
#END ledger while
END;
