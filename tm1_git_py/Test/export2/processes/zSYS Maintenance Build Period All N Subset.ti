sDimName_AllN = 'Period';
sHieName_AllN = 'Period';
sSubName_AllN = 'All N';

sMDX_AllN = 
'{
Filter(
  {
  TM1FilterByLevel(
    {
    TM1DrillDownMember(
      {
      [Period].[Period].[All N Element Period]
      },
      ALL,
      RECURSIVE)
    },
    0)
  },
  InStr([Period].[Period].CurrentMember.Name, "_Input") = 0 )
}';

if(HierarchySubsetExists(sDimName_AllN, sHieName_AllN, sSubName_AllN) = 0);
  HierarchySubsetCreate(sDimName_AllN, sHieName_AllN, sSubName_AllN);
endif;
HierarchySubsetMDXSet(sDimName_AllN, sHieName_AllN, sSubName_AllN, sMDX_AllN);
HierarchySubsetMDXSet(sDimName_AllN, sHieName_AllN, sSubName_AllN, '');

