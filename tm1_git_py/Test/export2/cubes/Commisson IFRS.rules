SKIPCHECK;

['Commission IFRS':'Commission IFRS':'Customer Service Incentive Cost Percentage','Commission IFRS Measure':'Value'] = C:
IF( ELLEV('Period',!Period)>0,
  DB('Commisson IFRS', !Version, DB('}ElementAttributes_Period', !Period, 'Last_M_Y'), !Commission IFRS, !Commission IFRS Measure),
Continue);

['Commission IFRS':'Commission IFRS':'Call Center Incentive Cost Percentage','Commission IFRS Measure':'Value'] = C:
IF( ELLEV('Period',!Period)>0,
  DB('Commisson IFRS', !Version, DB('}ElementAttributes_Period', !Period, 'Last_M_Y'), !Commission IFRS, !Commission IFRS Measure),
Continue);

# verzo szabályok
[]= N:
IF(ELISPAR('Version','Report',!Version) = 1,
STET,Continue);



# tranzakciós költségek Customer Service
['Commission IFRS':'Commission IFRS':'Customer Service Transaction Cost','Commission IFRS Measure':'Commission IFRS Measure':'Value'] =N:
DB('Commission', !Version, !Period, '1', 'Customer Service', 'Total Products', 'Total', 'Commission Total Cost', 'Value');

# tranzakciós költségek Call Center
['Commission IFRS':'Commission IFRS':'Call Center Transaction Cost','Commission IFRS Measure':'Commission IFRS Measure':'Value'] =N:
DB('Commission', !Version, !Period, '1', 'Call Center', 'Total Products', 'Total', 'Commission Total Cost', 'Value');

# ösztönző számítások
['Commission IFRS':'Commission IFRS':'Customer Service Incentive Cost','Commission IFRS Measure':'Commission IFRS Measure':'Value'] =N:
['Commission IFRS':'Commission IFRS':'Customer Service Transaction Cost']
*
['Commission IFRS':'Commission IFRS':'Customer Service Incentive Cost Percentage']/100;

['Commission IFRS':'Commission IFRS':'Call Center Incentive Cost','Commission IFRS Measure':'Commission IFRS Measure':'Value'] =N:
['Commission IFRS':'Commission IFRS':'Call Center Transaction Cost']
*
['Commission IFRS':'Commission IFRS':'Call Center Incentive Cost Percentage']/100;

