['ProcessLogEntryDelete','S Type'] = S:
Date(DayNo( Today(1) ) - DB('zSYS Maintenance Parameter','ProcessLogEntryDelete','N type'), 1);

['ActualMonth','S Type'] = S:
Subst(DB('zSYS Maintenance Parameter', 'ProcessLogEntryDelete', 'S Type'), 1, 4)|Subst(DB('zSYS Maintenance Parameter', 'ProcessLogEntryDelete', 'S Type'), 6, 2);

['ActivePhaseEnd','S Type'] = S:
IF(Subst(DB('zSYS Maintenance Parameter', 'ProcessLogEntryDelete', 'S Type'), 6, 2) @<> '12',
STR(NUMBR(Subst(DB('zSYS Maintenance Parameter', 'ProcessLogEntryDelete', 'S Type'), 1, 4)|Subst(DB('zSYS Maintenance Parameter', 'ProcessLogEntryDelete', 'S Type'), 6, 2))+1,6,0),
Subst(DB('zSYS Maintenance Parameter', 'ProcessLogEntryDelete', 'S Type'), 1, 4)|'01');