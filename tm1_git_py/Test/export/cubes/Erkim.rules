#SEEDER;
#TITLE: 
#VIEWS:
#LINKED:
SKIPCHECK;

#TITLE: 
#VIEWS:
#LINKED:
# verzió szabályok
[]= N:
IF(ELISPAR('Version','Report',!Version) = 1,
STET,Continue);

#TITLE: 
#VIEWS:
#LINKED:
# Mody 05 15
# az erkim attri alapján

['Erkim Measure':'Value'] =N:
IF( DB('}ElementAttributes_ledger', !ledger, 'sourceCube') @= 'Revenue',
  (-1) * DB('Revenue', !Version, !Period, !ledger, 'Total Products', 'Bevetel', 'Value'),
  Continue);

#TITLE: 
#VIEWS:
#LINKED:
['Erkim Measure':'Value'] =N:
IF( DB('}ElementAttributes_ledger', !ledger, 'sourceCube') @= 'EBITDA',
  DB('EBITDA', !Version, !Period, !ledger, 'Value'),
  Continue);

#TITLE: 
#VIEWS:
#LINKED:
['Erkim Measure':'Value'] =N:  
IF( DB('}ElementAttributes_ledger', !ledger, 'sourceCube') @= 'Direct Cost',
  DB('Direct Cost', !Version, !Period, !ledger, 'Cost', 'Value'),
  DB('Other Cost', !Version, !Period, !ledger, 'Cost', 'Value')
);

