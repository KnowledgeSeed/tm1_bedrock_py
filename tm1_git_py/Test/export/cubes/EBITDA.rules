
SKIPCHECK;

#felesleges főkönyvek kihagyása
['Ledger':'Ledger':'Revenue'] = STET;

['Ledger':'Ledger':'II. Koltseg'] = STET;



# verzió szabályok
[]= N:
IF(ELISPAR('Version','Report',!Version) = 1,
STET,Continue);

#Helyi iparuzesi ado
['ledger':'ledger':'86404','EBITDA Measure':'Value']= N:
IF( DIMIX('Period',!Period) >= DIMIX('Period',DB('zSYS Maintenance Parameter','ActualMonth','S Type')),
  DB('Taxes', !Version, !Period, 'Iparuzesi ado', 'Value'),
STET);

#innovacios jarulek
['ledger':'ledger':'86405','EBITDA Measure':'Value']= N:
IF( DIMIX('Period',!Period) >= DIMIX('Period',DB('zSYS Maintenance Parameter','ActualMonth','S Type')),
  DB('Taxes', !Version, !Period, 'Innovacios jarulek', 'Value'),
STET);

#Társasági adó
['ledger':'ledger':'89101','EBITDA Measure':'Value']= N:
IF( DIMIX('Period',!Period) >= DIMIX('Period',DB('zSYS Maintenance Parameter','ActualMonth','S Type')),
  DB('Taxes', !Version, !Period, 'Tarsasagi adokotelezettseg', 'Value'),
STET);

