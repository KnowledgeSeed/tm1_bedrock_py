['Channel Cost Report Measure':'Value'] = C:
IF( (ELLEV('Period',!Period)>0) & (DB('Channel Time Dependent Attributes', !Version, !Period, !Channel, 'Payment Type') @= 'EFI Cost'),
  DB('Channel Cost Report', !Version, !Period | '12', !Channel, !Channel Cost Report Measure),
Continue);
  
['Channel Cost Report Measure':'Comment']= N:
STET;

[]= N:
IF(ELISPAR('Version','Report',!Version) = 1,
STET,Continue);

['Channel Cost Report Measure':'Value'] = N:
IF( DB('Channel Time Dependent Attributes', !Version, !Period, !Channel, 'Payment Type') @= 'EFI Cost',
  DB('Channel Cost', !Version, !Period, !Channel, 'Total Products', DB('Channel Time Dependent Attributes', !Version, !Period, !Channel, 'Currency'), 'CPS', 'Value'),
  DB('Channel Cost', !Version, !Period, !Channel, 'Total Products',
    DB('Channel Time Dependent Attributes', !Version, !Period, !Channel, 'Currency'), DB('Channel Time Dependent Attributes', !Version, !Period, !Channel, 'Payment Type'), 'Value')
);

