#region Prolog

#****Begin: Generated Statements***
#****End: Generated Statements****

nLedgerIndex = 1;

sSub = 'Direct Cost Overview Import';
sDim = 'ledger';
sMdx = 
'{
	EXCEPT({
		TM1DRILLDOWNMEMBER({
			[ledger].[ledger].[Közvetlen költség]
		}, ALL, RECURSIVE)
	},{
		[ledger].[ledger].[Közvetlen költség]
	})
}';


SubsetCreateByMDX(sSub ,sMdx , sDim, 1);

nSubsetSize = SubsetGetSize('Ledger', 'Direct Cost Overview Import');
#endregion
#region Metadata
#****Begin: Generated Statements***
#****End: Generated Statements****
#endregion
#region Data
#****Begin: Generated Statements***
#****End: Generated Statements****
#Botond 08.14

sLedgerSubset = 'Direct Cost Overview Import';
sLedger = HierarchySubsetGetElementName('Ledger', 'Ledger', sLedgerSubset, nLedgerIndex);

#Ha egyezik a változó a subset elemével akkor végezze el a számítást
IF(vLedger @= sLedger);


IF(pMode = 3);  

sEditable = CellGetS('}ElementAttributes_Ledger', vLedger, 'Calculations And Logic');
IF( sEditable @= 'input' );
  
    ExecuteProcess('zSYS Analogic UI Direct Cost Input',
   'pMode', pMode,
   'pLedger', vLedger,
   'pValue', pValue,
   'pVersion', pVersion,
   'pUser', pUser
  );

ENDIF;
ENDIF;

IF(pMode = 4);  

