# Modos David 2024 02 02
# átírjuk a megfelelő attributumokat
# átmásoljuk az adott verzióról az adatokat

# kapott caption beállítása
DimName = 'Version';
HierName = 'Version';

# last fact
sLastFact = CellGetS('}ElementAttributes_Version', pSourceVersion, 'Last Fact Date');
ElementAttrPutS(sLastFact, DimName, HierName, pTargetVersion, 'Last Fact Date');


# Last Copy Date
sToday = Today(1);
ElementAttrPutS(sToday, DimName, HierName, pTargetVersion, 'Last Updated');


# Last Copy By
ElementAttrPutS(pUser, DimName, HierName, pTargetVersion, 'Last Updated By');



sProcessName = 'zSYS Analogic Operation Version Copy';

ExecuteProcess(sProcessName,
 'pSourceVersion', pSourceVersion,
 'pTargetVersion', pTargetVersion,
 'pClearTargetBeforeCopy', 1);
 
 
 
