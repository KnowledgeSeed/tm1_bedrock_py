#region Data
#****Begin: Generated Statements***
#****End: Generated Statements****

If(vXP @= '1P KTV');
    vXP = 'TV';
ELSEIF(vXP @= '1P NET');
    vXP = 'NET';
ELSEIF(vXP @= '1P TEL');
    vXP = 'TEL';
ELSEIF(vXP @= '2P KTV+NET');
    vXP = 'NET + TV';
ELSEIF(vXP @= '2P KTV+TEL');
    vXP = 'TV + TEL';
ELSEIF(vXP @= '2P TEL+NET');
    vXP = 'NET + TEL';
ELSEIF(vXP @= '3P');
    vXP = 'TEL + NET + TV';
EndIf;

CellPutN(vValue, sCubeName, pVersion, DELET(Subst(vDate, 1, 7),5,1), vXP, 'Value');
CellPutN(vValue, sCubeName, pVersion, DELET(Subst(vDate, 1, 7),5,1), vXP, 'Reset');

